class Solution {
  public String solution(int n) {
      String answer = "";
      for(int i=1;i<=n;i++){  //i = 0이 아닌 이유: 홀수 짝수 모두 아니고 조건에도 0이 없으므로
          if(i%2!=0) {        //홀수인 경우  
              answer+="수";
          }
          else {              //짝수인 경우
              answer+="박";
          }
      }
      return answer;
  }
}

//홀수 자리 :수
//짝수 자리 :박
