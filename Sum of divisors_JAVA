class Solution {
  public int solution(int n) {
      int answer = 0;           //약수의 합(sum)
      for(int i=1;i<=n;i++){
          if(n%i==0){           //n을 i로 나누어 약수를 구한다.
              answer += i;      //sum=sum+i
          }//(만족하는 i=약수)의 총합
      }
      return answer;            //약수의 합 출력
  }
}
