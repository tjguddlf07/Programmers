class Solution {
  public double solution(int[] arr) {
      int sum=0;            //배열의 합  
      double answer = 0;
      
      for(int i=0;i<arr.length;i++){ //배열의 크기를 모르므로 length
          sum+=arr[i];
      }
      answer=(double)sum/arr.length; //answer의 타입에 맞출것
      return answer;
  }
}

//arr.length : 배열의 길이 반환
